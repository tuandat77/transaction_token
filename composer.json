{
    "name": "ezdefi/ezdefi-send-token-poc-php",
    "description": "Ezdefi's PHP send token PoC",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ezdefi",
            "email": "support@ezdefi.com"
        }
